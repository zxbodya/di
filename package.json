{
  "name": "di1",
  "version": "0.1.5",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "description": "Simple but yet powerful, DI container for JavaScript",
  "keywords": [
    "DI",
    "dependency injection",
    "ioc",
    "container",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/di1"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "lib/di.js",
  "jsnext:main": "src/di.js",
  "scripts": {
    "lint": "eslint ./src/",
    "cover": "rimraf coverage && istanbul cover -x \"**/*.test.js\" ./node_modules/jasmine/bin/jasmine.js && npm run cover_remapping",
    "cover_remapping": "istanbul-sourcemap-report ./coverage/coverage.json --json.file=./coverage/coverage-remapped.json --html.dir=./coverage/coverage-remapped",
    "test": "jasmine",
    "coveralls": "cat ./coverage/*/coverage-remapped.lcov | ./node_modules/.bin/coveralls",
    "build": "rimraf lib && babel --presets es2015 --plugins transform-runtime src --out-dir lib --source-maps",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.11.4",
    "eslint": "^3.7.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.7.0",
    "istanbul": "^0.4.1",
    "istanbul-sourcemap-report": "^0.1.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "rimraf": "^2.4.4"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19"
  }
}
